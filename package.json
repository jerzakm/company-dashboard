{
	"name": "company-dashboard",
	"version": "0.0.1",
	"scripts": {
		"dev:only": "svelte-kit dev --host",
		"build:only": "svelte-kit build",
		"start": "svelte-kit start",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"prepare": "npm run vite-workaround && npm run smui-theme-light && npm run smui-theme-dark",
		"vite-workaround": "node _helper/vite-workaround.js",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o static/styles/tailwind-output.css -w",
		"tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o static/styles/tailwind-output.css",
		"dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
		"build": "npm run tailwind:build && npm run build:only"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.2.2",
		"@lukeed/uuid": "^2.0.0",
		"@material/theme": "^11.0.0",
		"@material/typography": "^11.0.0",
		"@prisma/client": "^2.22.1",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@types/knex": "^0.16.1",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"autoprefixer": "^10.3.7",
		"concurrently": "^6.3.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"postcss-cli": "^9.0.1",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"prisma": "^2.22.1",
		"sass": "^1.32.11",
		"smui-theme": "^4.1.0",
		"svelte": "^3.37.0",
		"svelte-image-gallery": "^1.3.0",
		"svelte-preprocess": "^4.0.0",
		"tailwindcss": "^2.2.16",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"vite": "^2.2.3"
	},
	"type": "module",
	"dependencies": {
		"@lottiefiles/lottie-player": "^1.4.1",
		"@smui/button": "^4.2.0",
		"@smui/card": "^4.2.0",
		"@smui/common": "^4.2.0",
		"@smui/data-table": "^4.2.0",
		"@smui/dialog": "^4.2.0",
		"@smui/drawer": "^4.2.0",
		"@smui/fab": "^4.2.0",
		"@smui/floating-label": "^5.0.0-beta.7",
		"@smui/form-field": "^5.0.0-beta.7",
		"@smui/icon-button": "^4.2.0",
		"@smui/line-ripple": "^5.0.0-beta.7",
		"@smui/list": "^4.2.0",
		"@smui/menu-surface": "^4.2.0",
		"@smui/paper": "^4.2.0",
		"@smui/select": "^4.2.0",
		"@smui/snackbar": "^4.2.0",
		"@smui/switch": "^5.0.0-beta.7",
		"@smui/textfield": "^4.2.0",
		"@smui/top-app-bar": "^4.2.0",
		"cookie": "^0.4.1",
		"dayjs": "^1.10.7",
		"deep-object-diff": "^1.1.0",
		"dotenv": "^10.0.0",
		"filepond": "^4.29.1",
		"filepond-plugin-file-validate-type": "^1.2.6",
		"filepond-plugin-image-exif-orientation": "^1.0.11",
		"filepond-plugin-image-preview": "^4.6.10",
		"knex": "^0.95.6",
		"svelte-filepond": "^0.2.0",
		"svelte-markdown": "^0.1.12",
		"svelte-tiny-virtual-list": "^1.1.6",
		"tedious": "^11.0.9"
	}
}
